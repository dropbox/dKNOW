{
  "corpus": "crates",
  "description": "Code corpus evaluation: 15 queries against sg's own codebase",
  "include_extensions": ["rs"],
  "include_globs": ["sg-*/src/*.rs", "sg-*/tests/*.rs", "sg/src/*.rs"],
  "queries": [
    {
      "query": "k-means clustering buckets centroids",
      "relevant": ["crates/sg-core/src/index.rs"],
      "description": "Should find the k-means clustering implementation"
    },
    {
      "query": "SQLite database documents embeddings store",
      "relevant": ["crates/sg-core/src/storage.rs"],
      "description": "Should find the SQLite storage module"
    },
    {
      "query": "text chunking overlap paragraphs split",
      "relevant": ["crates/sg-core/src/chunker.rs"],
      "description": "Should find the text chunker"
    },
    {
      "query": "XTR transformer BERT tokenizer embed",
      "relevant": ["crates/sg-core/src/embedder.rs"],
      "description": "Should find the embedder"
    },
    {
      "query": "MaxSim similarity scoring dot product",
      "relevant": ["crates/sg-core/src/embedder.rs"],
      "description": "Should find MaxSim implementation"
    },
    {
      "query": "file watcher notify debounce events",
      "relevant": ["crates/sg-daemon/src/watcher.rs"],
      "description": "Should find file watching code"
    },
    {
      "query": "Unix socket IPC server accept connections",
      "relevant": ["crates/sg-daemon/src/server.rs"],
